# Pipeworks Image Generator Configuration
# Copy this file to .env and adjust values as needed

# Model Adapter Settings
PIPEWORKS_DEFAULT_MODEL_ADAPTER=Z-Image-Turbo

# Model-Specific HuggingFace IDs
PIPEWORKS_ZIMAGE_MODEL_ID=Tongyi-MAI/Z-Image-Turbo
PIPEWORKS_QWEN_MODEL_ID=aidiffuser/Qwen-Image-Edit-2509

# Legacy Model Settings (for backward compatibility)
PIPEWORKS_MODEL_ID=Tongyi-MAI/Z-Image-Turbo

# General Model Settings
PIPEWORKS_TORCH_DTYPE=bfloat16
PIPEWORKS_DEVICE=cuda

# Z-Image-Turbo Specific
PIPEWORKS_NUM_INFERENCE_STEPS=9
PIPEWORKS_GUIDANCE_SCALE=0.0

# Default Generation Settings
PIPEWORKS_DEFAULT_WIDTH=1024
PIPEWORKS_DEFAULT_HEIGHT=1024

# Performance Optimizations
PIPEWORKS_ENABLE_ATTENTION_SLICING=false
PIPEWORKS_ENABLE_MODEL_CPU_OFFLOAD=false
PIPEWORKS_COMPILE_MODEL=false
PIPEWORKS_ATTENTION_BACKEND=default

# Paths
PIPEWORKS_MODELS_DIR=models
PIPEWORKS_INPUTS_DIR=inputs
PIPEWORKS_OUTPUTS_DIR=outputs
PIPEWORKS_CATALOG_DIR=catalog

# Gradio UI Settings
PIPEWORKS_GRADIO_SERVER_NAME=0.0.0.0
PIPEWORKS_GRADIO_SERVER_PORT=7860
PIPEWORKS_GRADIO_SHARE=false
